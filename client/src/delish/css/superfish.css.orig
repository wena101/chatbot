nav{ position:relative; margin: 24px 0 0px 0; height:90px;padding:34px 0 0 40px; background:url(../img/nav_bg.png) 0 0 no-repeat; z-index:99;}
.sf-menu ul {position:absolute;top:-999em;/* left offset of submenus need to match (see below) */}
.sf-menu ul li {width:100%}
.sf-menu li {float:left;position:relative}
.sf-menu>li>a{display:block;position:relative;font-size: 18px; line-height: 23px; letter-spacing: 1px; text-transform: uppercase; font-weight: 400; color:#ffffff; text-shadow: 1px 1px 1px rgba(0,0,0,0.88)}
.sf-menu>li>a:hover,.sf-menu>li.current>a,.sf-menu>li.sfHover>a{ color:#dbb25f;}
.second_part{ margin-left:300px;}
.sf-menu .li{ background:url(../img/nav_marker.png) 0 10px no-repeat;padding:0 0 0 20px;margin: 0 0 0 15px;}
/*================================>> 2 Level <<========================================*/
.sf-menu>li.sfHover>ul>span{width: 8px;height: 4px; display:block; background:url(../img/sub_span.png) no-repeat;position:absolute; top:-5px; left:48px;}
.sf-menu li:hover ul,.sf-menu li.sfHover ul{left:0;top:30px;width: 103px;padding:13px 0 4px 0px;/* match top ul list item height */z-index:99;border: 1px solid #6b451f; left:-1px; background:url(../img/sub_ul.png) repeat;}
.sf-menu li li{  padding: 0 0 1px 15px; margin-bottom:6px;}
.sf-menu li li a{font-size: 13px; line-height: 16px; color:#dfd7ab; text-shadow: 1px 1px 1px rgba(0,0,0,0.40); text-transform:uppercase;background:url(../img/sub_li.png) 0px bottom no-repeat; display:block; padding-bottom:7px;}
.sf-menu li li li a{ text-shadow: 0px 1px 1px #e5dbbb !important;}
.sf-menu li li a:hover, .sf-menu li.sfHover li.sfHover a{ color:#dbb25f;}
.sf-menu li ul li.last a{ background:none !important;}
.sf-menu li ul li a span{ display:block;width: 4px;height: 7px; position:absolute; right:44px; top:3px; background:url(../img/sub_li_span.png) no-repeat;}

.sf-menu .last_it>a{background:none !important;padding-bottom:0 !important;margin-bottom:0 !important;}
/*================================>> 3 Level <<========================================*/
.sf-menu li li ul{left:105px !important;top:-14px !important;width: 113px !important;padding:0 0 0 0;/* match top ul list item height */z-index:99; border: 1px solid #e5dbbb !important; background:url(../img/sub_sub_ul.png) repeat !important;}
.sf-menu li li li{}
.sf-menu li li li a{ color:#734418 !important; background:url(../img/sub_sub_a.png) 0 bottom no-repeat !important;}
.sf-menu li li li a:hover{color:#513418 !important;}
